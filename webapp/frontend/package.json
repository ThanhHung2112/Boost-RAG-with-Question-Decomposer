		"dev": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
		"check-types": "tsc --noEmit",
		"worker:test": "npx dotenv-cli -e .env -- npx tsx --watch app/workers/test.worker.ts",
		"key:generate": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('APP_KEY=' + buf.toString('hex')) });\"",
		"key:gen_session_pdf": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_PDF=' + buf.toString('hex')) });\"",
		"key:gen_session_hyperlink": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_HYPERLINK=' + buf.toString('hex')) });\"",
		"key:gen_session_job": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_JOB=' + buf.toString('hex')) });\"",
		"key:gen_session_message": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_MESSAGE=' + buf.toString('hex')) });\"",
		"key:generate_all": "node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('APP_KEY=' + buf.toString('hex')) });\" && node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_PDF=' + buf.toString('hex')) });\" && node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_HYPERLINK=' + buf.toString('hex')) });\" && node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_JOB=' + buf.toString('hex')) });\" && node -e \"require('crypto').randomBytes(32, function(ex, buf) { console.log('KEY_SESSION_MESSAGE=' + buf.toString('hex')) });\""
	},
